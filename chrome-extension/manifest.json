{
  "manifest_version": 3,
  "name": "自动填写短信验证码",
  "version": "1.0",
  "description": "自动获取并填写短信中的验证码",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "http://localhost:65530/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}